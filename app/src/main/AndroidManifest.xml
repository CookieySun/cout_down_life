<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kktyu.com.cout_down_life">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hourglass"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/hourglass"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="kktyu.com.cout_down_life.RemainingTimeActivity"></activity>
        <activity android:name="kktyu.com.cout_down_life.SetDate"></activity>
        <activity android:name="kktyu.com.cout_down_life.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>